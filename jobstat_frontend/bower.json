{
  "name": "jobstat",
  "dependencies": {
    "jquery": "latest",
    "bootstrap": "latest",
    "font-awesome": "latest",
    "datatables": "latest",
    "Chart.js": "1.1.1",
    "bootstrap-switch": "latest",
    "matchHeight": "latest",
    "select2": "latest",
    "animate.css": "latest",
    "checkbox3": "~0.1.1",
    "bootstrap-social": "latest",
    "jquery.easing": "1.3",
    "c3": "latest",
    "requirejs": "latest",
    "highcharts-release": "latest"
  },
  "private": true,
  "overrides": {
    "jquery": {
      "main": [
        "dist/jquery.min.js"
      ]
    },
    "bootstrap": {
      "main": [
        "dist/js/bootstrap.min.js",
        "dist/css/bootstrap.min.css",
        "fonts/*"
      ]
    },
    "font-awesome": {
      "main": [
        "css/font-awesome.min.css",
        "fonts/*"
      ]
    },
    "animate.css": {
      "main": [
        "animate.min.css"
      ]
    },
    "bootstrap-switch": {
      "main": [
        "dist/css/bootstrap3/bootstrap-switch.min.css",
        "dist/js/bootstrap-switch.min.js"
      ]
    },
    "checkbox3": {
      "main": [
        "dist/checkbox3.min.css"
      ]
    },
    "datatables": {
      "main": [
        "media/css/jquery.dataTables.min.css",
        "media/css/dataTables.bootstrap.css",
        "media/js/jquery.dataTables.min.js",
        "media/js/dataTables.bootstrap.min.js",
        "media/images/sort_both.png",
        "media/images/sort_desc.png",
        "media/images/sort_asc.png"
      ]
    },
    "Chart.js": {
      "main": [
        "Chart.js"
      ]
    },
    "select2": {
      "main": [
        "dist/js/select2.full.min.js",
        "dist/css/select2.min.css"
      ]
    },
    "bootstrap-social": {
      "main": [
        "bootstrap-social.css"
      ]
    },
    "jquery-easing": {
      "main": [
        "js/jquery.easing.min.js"
      ]
    },
    "c3": {
      "main": [
        "c3.css",
        "c3.min.js"
      ]
    },
    "requirejs": {
      "main": [
        "require.js"
      ]
    },
    "highcharts-release": {
      "main": [
        "highcharts.js",
        "highstock.js",
        "highcharts-more.js",
        "modules/exporting.src.js"
      ]
    }
  }
}
